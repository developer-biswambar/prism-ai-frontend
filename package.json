{
  "name": "prism-ai-frontend",
  "private": true,
  "version": "1.0.0",
  "description": "Prism AI Frontend - Modern React-based financial data processing interface",
  "type": "module",
  "author": "Your Organization",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/prism-ai-frontend.git"
  },
  "keywords": ["react", "vite", "financial", "data-processing", "frontend"],
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist build",
    "analyze": "npx vite-bundle-analyzer",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "test:ui": "playwright test",
    "test:ui:all": "node testing/test-runner.js all",
    "test:smoke": "node testing/test-runner.js smoke",
    "test:workflows": "node testing/test-runner.js workflows",
    "test:performance": "node testing/test-runner.js performance",
    "test:mobile": "node testing/test-runner.js mobile",
    "test:integration": "playwright test testing/e2e/",
    "test:integration:watch": "playwright test testing/e2e/ --ui",
    "test:report": "playwright show-report",
    "test:setup": "node testing/utils/test-data-setup.js",
    "test:debug": "playwright test --headed --debug",
    "test:visual": "node testing/visual-test-runner.js demo",
    "test:interactive": "node testing/visual-test-runner.js interactive", 
    "test:step": "node testing/visual-test-runner.js debug",
    "test:discover": "playwright test testing/e2e/simple-ui-test.spec.js --headed --reporter=line",
    "test:reconciliation": "playwright test testing/e2e/reconciliation-functionality.spec.js --headed --reporter=line",
    "test:reconciliation:debug": "playwright test testing/e2e/reconciliation-functionality.spec.js --headed --debug",
    "test:reconciliation:full": "playwright test testing/e2e/reconciliation-functionality.spec.js --headed --reporter=html",
    "test:reconciliation:focused": "playwright test testing/e2e/reconciliation-focused.spec.js --headed --reporter=line",
    "test:reconciliation:quick": "playwright test testing/e2e/reconciliation-focused.spec.js --headed --reporter=line --timeout=60000",
    "test:reconciliation:modal": "playwright test testing/e2e/reconciliation-modal-aware.spec.js --headed --reporter=line",
    "test:reconciliation:modal-debug": "playwright test testing/e2e/reconciliation-modal-aware.spec.js --headed --debug",
    "test:reconciliation:upload": "playwright test testing/e2e/reconciliation-upload-flow.spec.js --headed --reporter=line",
    "test:reconciliation:upload-debug": "playwright test testing/e2e/reconciliation-upload-flow.spec.js --headed --debug",
    "test:reconciliation:complete": "playwright test testing/e2e/reconciliation-complete-workflow.spec.js --headed --reporter=line --timeout=120000",
    "test:reconciliation:force": "playwright test testing/e2e/reconciliation-force-upload.spec.js --headed --reporter=line --timeout=60000",
    "test:reconciliation:correct": "playwright test testing/e2e/reconciliation-correct-workflow.spec.js --headed --reporter=line --timeout=90000",
    "docker:build": "docker build -t prism-ai-frontend .",
    "docker:build:dev": "docker build -f Dockerfile.dev -t prism-ai-frontend:dev .",
    "docker:run": "docker run -d --name prism-ai-frontend -p 3000:3000 prism-ai-frontend",
    "docker:run:dev": "docker run -d --name prism-ai-frontend-dev -p 5173:5173 prism-ai-frontend:dev",
    "docker:stop": "docker stop prism-ai-frontend && docker rm prism-ai-frontend",
    "docker:logs": "docker logs -f prism-ai-frontend"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "lucide-react": "^0.522.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^6.28.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@playwright/test": "^1.40.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.1",
    "vite": "^6.3.5",
    "vitest": "^1.0.0"
  }
}